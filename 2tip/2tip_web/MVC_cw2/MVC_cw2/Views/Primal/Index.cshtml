@model PrimalGenerator
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<h3>Widok akcji @ViewContext.RouteData.Values["action"] z kontrolera @ViewContext.RouteData.Values["controller"] </h3>

<form method="post">
    <div class="row m-2">
        <label class="col-4" for="limit" asp-for="Limit"></label>
        <input asp-for="Limit" type="number" class="col-4"/>
        <span class="col-4" asp-validation-for="Limit"></span>
    </div>
    <div class="row m-2">
        <input type="submit" class="btn btn-primary " value="Generuj"/>
    </div>
</form>

